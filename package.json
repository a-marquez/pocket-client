{
  "name": "pocket-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others 'npm run serve' 'npm run build:watch'",
    "serve": "node src/server",
    "build:watch": "parcel watch --out-dir build/js src/public/js/main.js",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "xo"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "env": "browser",
    "space": true,
    "semicolon": false,
    "rules": {
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "react/prop-types": 0,
      "capitalized-comments": 0
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "concurrently": "^3.5.1",
    "es6-enum": "^1.1.0",
    "eslint-config-xo": "^0.21.0",
    "eslint-config-xo-react": "^0.16.0",
    "eslint-plugin-react": "^7.7.0",
    "koa": "^2.5.0",
    "koa-route": "^3.2.0",
    "koa-session": "^5.8.1",
    "koa-static": "^4.0.2",
    "koa-stylus-parser": "^2.0.2",
    "node-fetch": "^2.1.1",
    "normalize.css.styl": "^4.1.1",
    "parcel-bundler": "^1.7.1",
    "ramda": "^0.25.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.11.1",
    "stylus-case": "github:a-marquez/stylus-case",
    "xo": "^0.20.3"
  }
}
